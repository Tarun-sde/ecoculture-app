# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Google Vision API Configuration
VITE_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Optional: Custom endpoints for services
VITE_WIKIPEDIA_API_URL=https://en.wikipedia.org/w/api.php
VITE_VISION_API_URL=https://vision.googleapis.com/v1/images:annotate

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_CACHE_EXPIRY_HOURS=24
VITE_MAX_IMAGE_SIZE_MB=5
VITE_COMPRESSION_QUALITY=0.8

# Error Reporting
VITE_ENABLE_ERROR_REPORTING=true
VITE_LOG_LEVEL=warn
